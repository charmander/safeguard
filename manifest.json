{
	"manifest_version": 2,
	"name": "Safeguard",
	"version": "0.4.0",
	"description": "Redirects plain HTTP to HTTPS.",
	"icons": {
		"128": "icon.svg"
	},
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"browser_action": {
		"default_icon": "icon.svg",
		"default_title": "Manage HTTP blocking"
	},
	"background": {
		"scripts": ["background.js"]
	}
}
